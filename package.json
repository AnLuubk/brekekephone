{
  "name": "BrekekePhone",
  "version": "3.0.0",
  "private": true,
  "homepage": "/phone",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "pret": "eslint --ignore-path=.gitignore --fix . && prettier --ignore-path=.gitignore --loglevel=silent --write \"**/*.{html,scss,json,md}\"",
    "pret-": "yarn pret && yarn pret-objc && yarn pret-java && yarn pret-xml && yarn imagemin",
    "pret-objc": "yarn git-ls | egrep -h '.(h|m)$' | xargs clang-format -i -style=file",
    "pret-java": "yarn git-ls | egrep -h '.(java)$' | xargs google-java-format -i",
    "pret-xml": "yarn git-ls | egrep -h '.(xml|xib|xccheme|xcworkspacedata|plist)$' | xargs -L1 bash -c 'xmllint --format --output $0 $0'",
    "imagemin": "yarn git-ls | egrep -h '.(png|jpg|gif|ico)$' | xargs -L1 bash -c 'imagemin $0 --out-dir $(dirname $0)'",
    "git-ls": "bash -c 'comm -3 <(git ls-files) <(git ls-files -d)'"
  },
  "dependencies": {
    "@mdi/js": "4.7.95",
    "@react-native-community/async-storage": "1.6.3",
    "@use-it/interval": "0.1.3",
    "brekekejs": "github:brekekesoftware/brekekejs#9b3e01cfb9191c4b6e8020d97a03006480bdfc44",
    "crypto-js": "3.1.8",
    "es6-shim": "0.35.5",
    "es7-shim": "6.0.0",
    "eventemitter3": "4.0.0",
    "jssip": "github:brekekesoftware/jssip#1e8446f930647bf9ebb1c2e642a38684db5729fd",
    "lodash": "4.17.15",
    "mobx": "5.15.2",
    "mobx-react": "6.1.5",
    "moment": "2.24.0",
    "polished": "3.4.2",
    "qs": "6.9.1",
    "react": "16.12.0",
    "react-native": "0.59.10",
    "react-native-callkeep": "3.0.8",
    "react-native-document-picker": "github:Elyx0/react-native-document-picker#v3",
    "react-native-exception-handler": "2.10.8",
    "react-native-fast-image": "6.1.1",
    "react-native-fcm": "github:brekekesoftware/react-native-fcm#f9ce0351f68db62b2295a55c44430d0066d0f3ae",
    "react-native-fs": "2.15.3",
    "react-native-full-image-picker": "github:brekekesoftware/react-native-full-image-picker#f515715c26b395ce0faa4af9e2840d63d682fc67",
    "react-native-general-actionsheet": "1.0.4",
    "react-native-incall-manager": "3.2.7",
    "react-native-iphone-x-helper": "1.2.1",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-progress-circle": "2.1.0",
    "react-native-splash-screen": "github:brekekesoftware/react-native-splash-screen#a78ee51c0f89652a2a223ff2bc4ec10928bf2c30",
    "react-native-svg": "9.13.6",
    "react-native-voip-push-notification": "2.0.0",
    "react-native-webrtc": "1.75.3",
    "shortid": "2.2.15",
    "url-parse": "1.4.7",
    "validatorjs": "3.18.1"
  },
  "devDependencies": {
    "react-art": "16.12.0",
    "react-device-detect": "1.11.14",
    "react-dom": "16.12.0",
    "react-native-svg-web": "1.0.1",
    "react-native-web": "0.11.7",
    "react-native-web-linear-gradient": "1.0.5"
  },
  "optionalDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-proposal-throw-expressions": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.8.3",
    "@babel/plugin-transform-react-jsx": "7.8.3",
    "@craco/craco": "5.6.3",
    "circular-dependency-plugin": "5.2.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "eslint-plugin-sort-destructure-keys": "1.3.3",
    "husky": "4.0.9",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "react-scripts": "3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "prettier",
      "simple-import-sort",
      "sort-destructure-keys"
    ],
    "rules": {
      "sort-imports": "off",
      "simple-import-sort/sort": 2,
      "import/first": 2,
      "import/newline-after-import": 2,
      "import/no-duplicates": 2,
      "import/no-extraneous-dependencies": 2,
      "prettier/prettier": 2,
      "react/jsx-sort-props": 2,
      "react/sort-prop-types": 2,
      "sort-destructure-keys/sort-destructure-keys": 2,
      "quotes": [
        2,
        "backtick"
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "browserslist": "ie 10-11"
}
