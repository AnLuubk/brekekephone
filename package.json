{
  "name": "BrekekePhone",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "lint": "eslint .",
    "pret": "js-import-sort --silent=1 --path src/**/*.js && prettier --ignore-path=.gitignore --loglevel=silent --write \"**/*.{html,scss,js,json,md}\""
  },
  "dependencies": {
    "brekekejs": "github:brekekesoftware/brekekejs",
    "crypto-js": "3.1.8",
    "eventemitter3": "4.0.0",
    "history": "4.9.0",
    "immutable": "github:brekekesoftware/immutable",
    "jssip": "github:brekekesoftware/jssip#brekeke-debug",
    "lodash": "4.17.14",
    "mobx": "5.11.0",
    "mobx-react": "6.1.1",
    "mobx-react-router": "4.0.7",
    "prop-types": "15.7.2",
    "qs": "6.7.0",
    "react": "16.8.6",
    "react-art": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "0.59.9",
    "react-native-document-picker": "github:Elyx0/react-native-document-picker#v3",
    "react-native-exception-handler": "2.10.8",
    "react-native-fcm": "github:brekekesoftware/react-native-fcm",
    "react-native-fs": "2.13.3",
    "react-native-full-image-picker": "github:brekekesoftware/react-native-full-image-picker",
    "react-native-general-actionsheet": "1.0.4",
    "react-native-incall-manager": "3.2.4",
    "react-native-iphone-x-helper": "1.2.1",
    "react-native-keep-awake": "4.0.0",
    "react-native-loud-speaker": "1.0.4",
    "react-native-modal-selector": "1.0.3",
    "react-native-permissions": "1.1.1",
    "react-native-progress-circle": "2.0.1",
    "react-native-splash-screen": "github:brekekesoftware/react-native-splash-screen",
    "react-native-voip-push-notification": "1.1.4",
    "react-native-web": "0.11.5",
    "react-native-webrtc": "1.69.2",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "redux": "4.0.4",
    "redux-model": "github:brekekesoftware/redux-model",
    "redux-persist": "5.10.0",
    "shortid": "2.2.14",
    "striptags": "3.1.1",
    "sweetalert2": "^8.13.6",
    "url-parse": "1.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@craco/craco": "^5.2.4",
    "circular-dependency-plugin": "^5.0.2",
    "js-import-sort": "1.2.0",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "react-scripts": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "babel-eslint",
    "settings": {
      "react": {
        "version": "latest"
      }
    }
  },
  "eslintIgnore": [
    "node_modules/**",
    "android/**",
    "ios/**",
    "build/**"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
