    "android/**",
    "ios/**",
    "build/**",
    "jsonrpc.js",
    "pal.js",
    "ucclient.js",
    "webrtcclient.js"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
